  "description": "loft-enterprise js client",
  "dependencies": {
    "@kubernetes/client-node": "1.2.0",
    "js-cookie": "3.0.1",
    "json-merge-patch": "1.0.2"
  },
  "devDependencies": {
    "dts-bundle-generator": "8.0.1",
    "esbuild": "0.25.0"
  }
}
