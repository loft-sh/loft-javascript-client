{
  "name": "@loft-enterprise/client",
  "version": "0.1.0",
  "private": true,
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "module": "lib/index.esm.js",
  "scripts": {
    "build": "node bin/build.js",
